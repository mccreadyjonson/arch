URxvt.font: xft:Misc Tamsyn:style=Regular:size=15
URxvt.boldFont: xft:Misc Tamsyn:style=Bold:size=15
!URxvt.letterSpace: -1
URxvt.scrollBar: false
!*foreground:	white
!*background:	black


URxvt.perl-ext-common: default,matcher,resize-font
!needs urxvt-perls installed
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
!URxvt.depth: 32
!URxvt.background: [90]#000000
!URxvt.transparent:	true
!URxvt.shading:		5
URxvt.cursorColor:	magenta
URxvt.intensityStyles: 	false

URxvt.resize-font.smaller:	C-u
URxvt.resize-font.bigger:	C-i

