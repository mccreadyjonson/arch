URxvt.font: xft:xos4 Terminus:style=Regular:size=15
Rxvt.boldFont: xft:xos4 Terminus:style=Bold:size=15
!URxvt.letterSpace: -1
URxvt.scrollBar: false
!*foreground:	white
!*background:	black


URxvt.perl-ext-common: resize-font
!URxvt.depth: 32
!URxvt.background: [90]#000000
!URxvt.transparent:	true
!URxvt.shading:		5
URxvt.cursorColor:	magenta
URxvt.intensityStyles: 	false

URxvt.resize-font.smaller:	C-u
URxvt.resize-font.bigger:	C-i

